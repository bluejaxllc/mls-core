# --- Frontend Production Environment Variables ---

# NextAuth Configuration
# The base URL of the application in production (e.g., https://mls-governance-web.vercel.app)
NEXTAUTH_URL=
# A long, random string used to secure the NextAuth session
NEXTAUTH_SECRET=

# API Configuration
# The base URL of the backend API (e.g., https://mls-core-api.railway.app)
NEXT_PUBLIC_API_URL=

# Blue Jax OAuth2 (GHL)
# Get these from your Blue Jax / GHL Marketplace App Settings
BLUE_JAX_CLIENT_ID=
BLUE_JAX_CLIENT_SECRET=
